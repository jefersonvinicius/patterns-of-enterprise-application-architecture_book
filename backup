import java.util.ArrayList;
import java.util.List;

public class Main {
  
  static interface VirtualListLoader {
    List load();
  }
  
  static class Supplier {
    private List products;
  }
  
  static class Product {
    private Long id;
    private String name;
    private Double price;
    
    Product(Long id, String name, Double price) {
      this.id = id;
      this.name = name;
      this.price = price;
    }
  }
  
  public static class ProductsMapper {
    static ProductsMapper create() {
      return new ProductsMapper();
    }
    
    List<Product> findForSupplier(Long supplierId) {
      return new ArrayList();
    }
  }
  
  class SupplierMapper {
    public static class ProductLoader implements VirtualListLoader {
      private Long supplierId;
      
      ProductLoader(Long id) {
        this.supplierId = id;
      }
      
      public List load() {
        return ProductsMapper.create().findForSupplier(this.supplierId);
      }
    }
  }
  
  public static void main(String[] args) {
    System.out.println("Hello");
  }
}